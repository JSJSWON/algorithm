   "source": [
    "def solution(n, money):\n",
    "    money.sort()\n",
    "    dp = [0 for _ in range(n+1)]\n",
    "    dp[0] = 1\n",
    "    for m in money:\n",
    "        for i in range(m, n+1):\n",
    "            if i >= m:\n",
    "                dp[i] += dp[i - m]\n",
    "    return dp[-1] % 1000000007"
   ]
