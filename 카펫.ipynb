   "source": [
    "def solution(brown, yellow):\n",
    "    answer = []\n",
    "    \n",
    "    b = (brown / 2 + 2)\n",
    "    \n",
    "    answer.append(int((b + (b**2 - 4 * (brown + yellow))**0.5) // 2))\n",
    "    answer.append(int((b - (b**2 - 4 * (brown + yellow))**0.5) // 2))\n",
    "    \n",
    "    return answer"
   ]
